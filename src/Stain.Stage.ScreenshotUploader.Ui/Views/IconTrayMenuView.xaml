<Window x:Class="Stain.Stage.ScreenshotUploader.Ui.Views.IconTrayMenuView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Stain.Stage.ScreenshotUploader.Ui.Views"
        mc:Ignorable="d"
        xmlns:prism="http://prismlibrary.com/" 
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="IconTrayMenuView" Height="450" Width="800">
    <Window.Resources>
        <ContextMenu x:Key="NotifierContextMenu"
                    Placement="MousePoint">
            <MenuItem Header="Open" Click="Menu_Open"/>
            <MenuItem Header="New Windowed Screenshot" Click="Menu_NewWindowedScreenshot"/>
        </ContextMenu>
    </Window.Resources>
</Window>
